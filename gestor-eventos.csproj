<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <RootNamespace>gestor_eventos</RootNamespace>
    <UserSecretsId>f1a71443-8ee9-43b2-9934-2b030f5ae70b</UserSecretsId>
    <!-- Suprimir warnings menos críticos que no afectan la funcionalidad -->
    <NoWarn>$(NoWarn);CS8618;CS8602;CS8603;CS8600;CS8601;CS8604;CS8625;MVC1001;MVC1002</NoWarn>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="DotNetEnv" Version="3.1.1" />
    <PackageReference Include="EPPlus" Version="7.0.0" />
    <PackageReference Include="System.Drawing.Common" Version="9.0.6" />
  </ItemGroup>

</Project>
